    margin-top: 6rpx;
}

.header-text .none {
    display: none;
}

.header-text {
    position: relative;
}

.header-text text {
    width: 160px;
    font-size: 18px;
    color: #ffffff;
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.header-right {
    font-size: 36rpx;
    flex: 1;
}

.header-right .dec {
    font-size: 28rpx;
    opacity: 0.8;
}

.arrow image {
    width: 20rpx;
    height: 38rpx;
}

.list-img {
    position: relative;
}

.list-img .tab {
    position: absolute;
    top: -15rpx;
    right: -48rpx;
    color: #ffffff;
    background: #fc774a;
    font-size: 18rpx;
    text-align: center;
    width: 100rpx;
    height: 30rpx;
    line-height: 30rpx;
    border-radius: 15rpx;
}

.list-img .tab::after {
    content: "";
    height: 0;
    width: 0;
    border: 4rpx solid transparent;
    border-top: 4rpx solid #fc774a;
    position: absolute;
    bottom: -6rpx;
    right: 48rpx;
}

.tab {
    position: absolute;
    top: 2rpx;
    color: #ffffff;
    background: #fc774a;
    font-size: 18rpx;
    text-align: center;
    width: 100rpx;
    height: 30rpx;
    line-height: 30rpx;
    border-radius: 15rpx;
}

.tab::after {
    content: "";
    height: 0;
    width: 0;
    border: 4rpx solid transparent;
    border-top: 4rpx solid #fc774a;
    position: absolute;
    bottom: -6rpx;
    right: 42rpx;
}

.header-content .logo {
    width: 80px;
    height: 80px;
    border: 4rpx solid #ffffff;
    border-radius: 8rpx;
    background: #ffffff;
    box-sizing: border-box;
    margin-right: 20rpx;
}

