page {
    background: rgba(240,240,240,0.80);
}

.home {
    padding-bottom: 118rpx;
}

.header-content {
    display: flex;
    align-items: center;
    background: #4C9AFA;
    color: #ffffff;
    padding: 57rpx 30rpx 40rpx;
}

.header-content input {
    text-align: center;
    color: #ffffff;
    width: 160px;
    font-size: 36rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.header-name {
